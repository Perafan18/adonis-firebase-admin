{
  "name": "adonis-firebase-admin",
  "version": "1.1.1",
  "description": "Firebase for AdonisJS",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perafan18/adonis-firebase-cloud-messaging.git"
  },
  "keywords": [
    "AdonisJs",
    "Adonis",
    "Firebase",
    "FCM",
    "NodeJS"
  ],
  "author": "Pedro Peraf√°n Carrasco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Perafan18/adonis-firebase-cloud-messaging/issues"
  },
  "homepage": "https://github.com/Perafan18/adonis-firebase-cloud-messaging#readme",
  "dependencies": {
    "firebase": "^5.5.6",
    "firebase-admin": "^5.13.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "dotenv": "^5.0.1",
    "faker": "^4.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^11.9.0",
    "semver": "^5.6.0",
    "standard": "^11.0.1"
  },
  "nyc": {
    "exclude": [
      "bin"
    ]
  },
  "standard": {
    "globals": [
      "use"
    ]
  }
